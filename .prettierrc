{
  "semi": false,
  "tabWidth": 2,
  "printWidth": 120,

  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^(@api)/(.*)$",
    "^(@store)/(.*)$",
    "^(@modals|@pages)/(.*)$",
    "^(@hocs|@hooks)/(.*)$",
    "^(@components|@examples|@icons)/(.*)$",
    "^(@utils|@styles|@constants)/(.*)$",
    "^@/lib/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true
}
